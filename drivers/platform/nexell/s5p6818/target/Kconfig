config ARCH_S5P6818_TARGET
	bool "s5p6818 Target Feactures"
	default n
	depends on ARCH_S5P6818

choice
    prompt "Type"
    depends on ARCH_S5P6818_TARGET

   	config ARCH_S5P6818_SVT
		bool "SVT: validation board"
		help
   	 		Support for the validation board (SVT).

   	config ARCH_S5P6818_ASB
		bool "ASB: validation board"
		help
   	 		Support for the ASB board (ASB).
endchoice

if ARCH_S5P6818_SVT
source "drivers/platform/nexell/s5p6818/target/svt/Kconfig"
endif

